{
  "name": "bojler",
  "version": "1.0.0",
  "description": "Quickly generate boilerplate files for your projects",
  "keywords": [
    "bojler",
    "boil",
    "boiler",
    "boilerplate",
    "generator",
    "generate",
    "snippets",
    "vscode"
  ],
  "license": "MIT",
  "source": "src/index.ts",
  "main": "build/index.js",
  "types": "build/types.d.ts",
  "engines": {
    "node": ">=16"
  },
  "bin": {
    "bojler": "./bin/bojler.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kamilmielnik/bojler.git"
  },
  "author": {
    "name": "Kamil Mielnik",
    "email": "kamil@kamilmielnik.com",
    "url": "https://kamilmielnik.com"
  },
  "bugs": {
    "url": "https://github.com/kamilmielnik/bojler/issues"
  },
  "scripts": {
    "build": "parcel build",
    "clean": "rimraf build",
    "prebuild": "npm run clean",
    "prerelease": "npm run prettier && npm run build",
    "prettier": "prettier --list-different './src/**/*.{js,jsx,ts,tsx}'",
    "prettier:fix": "npm run prettier -- --write --no-list-different",
    "release": "np --no-tests"
  },
  "targets": {
    "main": {
      "distDir": "./build"
    }
  },
  "dependencies": {
    "mkdirp": "^2.1.5"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.8.3",
    "@parcel/transformer-typescript-types": "^2.8.3",
    "np": "^7.6.3",
    "parcel": "^2.8.3",
    "prettier": "^2.8.4",
    "rimraf": "^4.4.0",
    "typescript": "^4.9.5"
  }
}
